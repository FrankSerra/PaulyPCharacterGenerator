<p id="notice"><%= notice %></p>

<h1>Weapon-Style Combos</h1>

<table>
  <thead>
    <tr>
      <th>Weapon</th>
      <th>Style</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weapon_offense_type_combos.each do |weapon_offense_type_combo| %>
      <tr>
        <td><%= weapon_offense_type_combo.weapon.name %></td>
        <td><%= weapon_offense_type_combo.offense_type.name %></td>
        <td><%= link_to 'Show', weapon_offense_type_combo %></td>
        <td><%= link_to 'Edit', edit_weapon_offense_type_combo_path(weapon_offense_type_combo) %></td>
        <td><%= link_to 'Destroy', weapon_offense_type_combo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weapon Offense Type Combo', new_weapon_offense_type_combo_path %>
